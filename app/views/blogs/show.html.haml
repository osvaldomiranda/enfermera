.container{:role => "main"}
  .col-md-12
    .panel.panel-default
      .panel-heading
        %h3.panel-title Noticias
      .panel-body
        .container-fluid
          %p#notice= notice

          %p
            %b Titulo:
            = @blog.titulo
          %p
            %b Descripcion:
            = raw(@blog.descripcion)
          %p
            %b Imagen:
            = @blog.imagen
          %p
            %b Documento:
            = @blog.documento
          %p
            %b Estado:
            = @blog.estado


          = link_to 'Continuar', blogs_path
